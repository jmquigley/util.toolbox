{
  "name": "util.toolbox",
  "version": "0.0.58",
  "author": "jmquigley <jmquigley@outlook.com>",
  "description": "A set of functions used for build and testing across projects",
  "main": "./index.js",
  "types": "./index.d.ts",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jmquigley/util.toolbox"
  },
  "bugs": {
    "url": "https://github.com/jmquigley/util.toolbox/issues"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "util.constants": "^0.0.10",
    "util.rstrip": "0.0.35",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/node": "^9.6.6",
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "nyc": "^11.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "util.pkgwrap": "^0.0.91"
  },
  "pkgwrap": {
    "cleanup": [
      "docs",
      "index.d.ts",
      "index.js",
      "./test/tests.d.ts",
      "./test/tests.js",
      "./test/helpers/index.d.ts",
      "./test/helpers/index.js"
    ]
  },
  "scripts": {
    "all": "yarn install && yarn run build && yarn run docs && yarn test && yarn run report",
    "build": "yarn run clean && yarn run lint && pkgwrap --build",
    "clean": "pkgwrap --clean",
    "coverage": "pkgwrap --coverage",
    "docs": "pkgwrap --docs",
    "lint": "pkgwrap --lint",
    "prepublishOnly": "yarn run all",
    "report": "pkgwrap --reporting",
    "test": "pkgwrap --testing --ava"
  }
}
